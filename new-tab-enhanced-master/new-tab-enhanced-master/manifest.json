{
  "applications": {
    "gecko": {
      "id": "enhanced-new-tab@isaac-newt.com",
       "strict_min_version": "54.0"
     }
  },

  "manifest_version": 2,
  "name": "New Tab Enhanced",
  "version": "1.6.0",

  "description": "A better new tab experience",

  "icons": {
    "48": "new-tab-enhanced.png"
  },

  "options_ui": {
    "page": "options/options.html"
  },

  "permissions": [
    "storage", "identity", "topSites", "bookmarks"
  ],

  "chrome_url_overrides": {
    "newtab": "newtab/newtab.html"
  }
}
